<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mill</title>
    <link rel="stylesheet" type="text/css" href="~/css/semantic.min.css">
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/semantic.min.js"></script>


</head>
    <body>
        <div class="ui visible left demo vertical inverted sidebar labeled icon menu">
            <a class="item" asp-area="" asp-controller="Home" asp-action="Index">
                <i class="home icon"></i>
                Home
            </a>
            <a class="item" asp-area="" asp-controller="Home" asp-action="Generate">
                <i class="audio description icon"></i>
                Generate
            </a>
            <a class="item" asp-area="" asp-controller="Home" asp-action="Privacy">
                <i class="question circle outline icon"></i>
                Privacy
            </a>
            <a class="item" asp-area="" asp-controller="Home" asp-action="Exchange">
                <i class="shopping basket icon"></i>
                Exchange
            </a>
        </div>
        <div id="viewport" style="margin-left:120px; margin-top:20px;">@RenderBody()</div>
    </body>

</html>
